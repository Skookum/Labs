@import 'normalize';

html
  background #ddd url(images/labs_bg.png)
body
  width 90%
  margin 10px auto
  background-color #fff
  border 1px solid #999
  border-radius 5px
  padding 20px
  position relative

#logo
  a
    background url(images/skookum.png) no-repeat
    display block
    height 86px
    text-indent -999em
    overflow hidden

.notice
  background #f7f4ba
  border-radius 3px
  border 1px solid #ddd588
  font-weight bold
  margin -2em auto 0
  padding .375em .75em
  text-align center
  width 50%

